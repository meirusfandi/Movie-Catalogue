<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    package="com.meirusfandi.cataloguemovie">    <uses-permission android:name="android.permission.INTERNET"/>    <permission android:name="com.meirusfandi.cataloguemovie.WRITE_DATABASE"        android:protectionLevel="normal"/>    <permission android:name="com.meirusfandi.cataloguemovie.READ_DATABASE"        android:protectionLevel="normal"/>    <application        android:allowBackup="true"        android:icon="@mipmap/ic_launcher"        android:label="@string/app_name"        android:roundIcon="@mipmap/ic_launcher_round"        android:supportsRtl="true"        android:theme="@style/AppTheme">        <activity            android:name=".views.main.MainActivity"            android:label="@string/app_name">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity android:name=".views.detail.DetailActivity"></activity>        <provider            android:authorities="com.meirusfandi.cataloguemovie"            android:name=".provider.MovieProvider"            android:exported="true"            android:readPermission="com.meirusfandi.cataloguemovie.READ_DATABASE"            android:writePermission="com.meirusfandi.cataloguemovie.WRITE_DATABASE"/>    </application></manifest>