<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    package="com.meirusfandi.cataloguemovie">    <uses-permission android:name="android.permission.INTERNET" />    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />    <uses-permission android:name="android.permission.VIBRATE" />    <uses-permission android:name="android.permission.WAKE_LOCK" />    <permission        android:name="com.meirusfandi.cataloguemovie.WRITE_DATABASE"        android:protectionLevel="normal" />    <permission        android:name="com.meirusfandi.cataloguemovie.READ_DATABASE"        android:protectionLevel="normal" />    <application        android:allowBackup="true"        android:icon="@mipmap/ic_launcher"        android:label="@string/app_name"        android:roundIcon="@mipmap/ic_launcher_round"        android:supportsRtl="true"        android:theme="@style/AppTheme">        <activity            android:name=".views.main.MainActivity"            android:label="@string/app_name">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity android:name=".views.detail.DetailActivity" />        <activity android:name=".notification.SettingsActivity"            android:parentActivityName=".views.main.MainActivity"/>        <activity android:name=".notification.CustomActivity" />        <provider            android:name=".provider.MovieProvider"            android:authorities="com.meirusfandi.cataloguemovie"            android:exported="true"            android:readPermission="com.meirusfandi.cataloguemovie.READ_DATABASE"            android:writePermission="com.meirusfandi.cataloguemovie.WRITE_DATABASE" />        <receiver android:name=".widget.FavoriteWidget">            <intent-filter>                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />            </intent-filter>            <meta-data                android:name="android.appwidget.provider"                android:resource="@xml/favorite_widget_info" />        </receiver>        <service android:name=".widget.StackViewsService"            android:permission="android.permission.BIND_REMOTEVIEWS"/>    </application></manifest>